# Jamit Scaffolds
#-----------------

class {0}Case(Case):
    def populate(self, line):
        ### You have to implement this
        return

    def isDone(self):
        ### You have to implement this
        return

    def __str__(self):
        ### You have to implement this
        return
		
class {0}Solver(Solver):
    def __init__(self):
        ### You have to implement this
        return
    
    def solve(self, case):
        ### You have to implement this
        return

#--------------
# Command Line
#--------------

if __name__ == "__main__":

    cases = None

    if len(sys.argv) < 2:
        print("Please supply an input data file")
        exit()
    
    # read in file from command line parameter
    filename = sys.argv[1]
    file = open(filename,"r")

    # parse input file into cases list
    while file:
        case = {0}Case()
        case.populate(file.readline())
        cases.add(case)
    
    file.close()

    # initialize solver
    solver = {0}Solver()

    # solve cases
    for case in cases:
        result = solver.solve(case)
        print(result)

